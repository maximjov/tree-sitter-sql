{
  "name": "@maximjov/tree-sitter-sql",
  "version": "0.3.8",
  "description": "SQL grammar for tree-sitter, but it is has more better grammer",
  "main": "bindings/node/index.js",
  "types": "bindings/node",
  "scripts": {
    "install": "npx --yes --package=tree-sitter-cli@v0.24.7 -- tree-sitter generate && node-gyp-build",
    "prestart": "npx --yes --package=tree-sitter-cli@v0.24.7 -- tree-sitter build --wasm",
    "start": "npx --yes --package=tree-sitter-cli@v0.24.7 -- tree-sitter playground",
    "release": "commit-and-tag-version",
    "test": "node --test bindings/node/*_test.js",
    "prebuildify": "prebuildify --napi --strip"
  },
  "author": "maximjov - forked from Derek Stride",
  "license": "MIT",
  "dependencies": {
    "generate": "^0.14.0",
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "commit-and-tag-version": "^12.0.0",
    "node-gyp": "^10.0.1",
    "prebuildify": "^6.0.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "gypfile": true,
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximjov/tree-sitter-sql.git"
  },
  "bugs": {
    "url": "https://github.com/maximjov/tree-sitter-sql/issues"
  },
  "homepage": "https://github.com/maximjov/tree-sitter-sql#readme",
  "commit-and-tag-version": {
    "skip": {
      "tag": true
    }
  },
  "keywords": [
    "parser",
    "sql"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ]
}
